{
  "$schema": "http://json.schemastore.org/ide.host",
  "order": 200,
  "icon": "ide/WebAPI.png",
  "supportsDocker": true,
  "symbolInfo": [
    {
      "id": "EventBundle",
      "name": {
        "text": "The name of the bundle of the events definitions",
        "overrideDefaultText": true
      },
      "description": {
        "text": "The name of the events-bundle, i.e. Foo will generate IFoo interface for the interface holding the events definitions",
        "overrideDefaultText": true
      },
      "isVisible": true,
      "defaultValue": "ProductCycle",
      "persistenceScope": "templateGroup"
    },
    {
      "id": "DisableOpenAPI",
      "name": {
        "text": "Enable _OpenAPI support",
        "overrideDefaultText": true
      },
      "description": {
        "text": "Enables OpenAPI (Swagger) support",
        "overrideDefaultText": true
      },
      "invertBoolean": true,
      "isVisible": true,
      "defaultValue": "true",
      "persistenceScope": "templateGroup"
    },
    {
      "id": "DisableGitHubCI",
      "name": {
        "text": "Enable GitHub CI",
        "overrideDefaultText": true
      },
      "description": {
        "text": "Use .github folder with CI definitions",
        "overrideDefaultText": true
      },
      "invertBoolean": true,
      "isVisible": true,
      "defaultValue": "true",
      "persistenceScope": "templateGroup"
    },
    {
      "id": "DisableTelemetry",
      "name": {
        "text": "Enable Telemetry support",
        "overrideDefaultText": true
      },
      "description": {
        "text": "Enable Open Telemetry configuration",
        "overrideDefaultText": true
      },
      "invertBoolean": true,
      "isVisible": true,
      "defaultValue": "true",
      "persistenceScope": "templateGroup"
    },
    {
      "id": "Producer",
      "name": {
        "text": "Generate producer code & setting",
        "overrideDefaultText": true
      },
      "isVisible": true,
      "defaultValue": "true",
      "persistenceScope": "templateGroup"
    },
    {
      "id": "Consumer",
      "name": {
        "text": "Generate consumer code & setting",
        "overrideDefaultText": true
      },
      "isVisible": true,
      "defaultValue": "true",
      "persistenceScope": "templateGroup"
    }
  ]
}
